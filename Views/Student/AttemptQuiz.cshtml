@{
    ViewBag.Title = "AttemptQuiz";
    Layout = "~/Views/Shared/StudentLayout2.cshtml";
}


<body>
    <div>

        <div class="row">
            <div class="col-sm-2">
            </div>
            <div class="col-sm-4">
                <h1>Select Quiz</h1>
            </div>
        </div>
        <br /><br />
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-md-offset-1">
                    <div class="login-panel panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Select Quiz</h3>
                        </div>
                        <div class="panel-body">
                            <form id="Announce" role="form" action="StartQuiz" method="get" enctype="multipart/form-data">
                                <fieldset>
                                    <select class="form-control" name="queID" form="Announce">
                                        @foreach (int id in Model)
                                        {
                                            <option>@id</option>
                                        }
                                    </select>
                                    <!-- Change this to a button or input when using this as a form -->
                                    <button type="submit" class="btn btn-lg btn-success btn-block">Start Quiz</button>
                                </fieldset>
                            </form>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br /><br />

    </div>
</body>